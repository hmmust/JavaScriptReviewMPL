 <%- include("../partials/head.ejs",{PageTitle:title}) %>
 <%- include("../partials/nav.ejs")%>
 <h1>Product Details</h1>
 <table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Price (JD)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
         <% for([i,p] of products.entries()){ %>
            <tr>
                <td><%= p.name%></td>
                <td><%= p.price%></td>
                <td><a href="/shop/deleteproduct/<%= i %>">delete</a></td>
            </tr>
        <%}%>
    </tbody>
</table>
<%- include("../partials/footer.ejs") %>